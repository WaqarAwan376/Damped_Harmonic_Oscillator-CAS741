{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["warn", "always"],
    "quotes": ["warn", "double"],
    "space-before-function-paren": "off",
    "react/prop-types": "off"
  }
}
